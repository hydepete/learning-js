const number1 = 15;
const number2 = 20;

function compareNumbers(numberOne, numberTwo){
    let message;

    if(numberOne > numberTwo){

        message = "Number one is greater than number two";

    } else if(numberOne === numberTwo){

        message = "Both numbers are equal";

    } else {

        message = "Number two is greater than number one";

    }

    return message;
}

console.log(compareNumbers(number1, number2));


//lab work

const computerChoice = "Paper";
//change this to either rock or scissors
const userChoice = "Paper";

function playGame(choice) {
    let message;

    if(choice===computerChoice){
        message = "It is a tie!";
    }
    else if(choice==="rock"){
        message = "User loses!";
    }
    else{
        message = "User wins!"
    }
    return message;
}
console.log(playGame(userChoice));